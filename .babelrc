{
  "presets": [
    "react",
    ["env", { 
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"],
      },
      "modules": false,
      "debug": false,
    }],
    "stage-0",
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    "babel-plugin-transform-regenerator",
    "babel-plugin-transform-runtime",
    "babel-plugin-transform-es2015-modules-commonjs",
  ],
}